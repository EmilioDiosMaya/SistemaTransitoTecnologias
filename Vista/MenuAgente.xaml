                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#FFBAFD07" Offset="1"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
        <Label Content="Bienvenido oficial Bernardo" HorizontalAlignment="Left" Margin="303,59,0,0" VerticalAlignment="Top" Width="163" HorizontalContentAlignment="Center"/>
        <Button Content="Generar reporte" HorizontalAlignment="Left" Margin="98,317,0,0" VerticalAlignment="Top" Width="100"/>
        <Button Content="Ver dictamen de reporte" HorizontalAlignment="Left" Margin="566,317,0,0" VerticalAlignment="Top" Width="139"/>
        <Button Content="Historial de reportes" HorizontalAlignment="Left" Margin="331,317,0,0" VerticalAlignment="Top" Width="117"/>

    </Grid>
